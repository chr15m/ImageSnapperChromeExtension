{
	"name": "ImageSnapper",
	"description": "Download images and add them to an RSS feed.",
	"version": "0.1.0",
	"permissions": [
		"contextMenus",
		"downloads"
	],
	"background": {
		"persistent": false,
		"scripts": ["jquery-1.11.1.min.js", "common.js", "background.js"]
	},
	"manifest_version": 2,
	"icons": {
		"16": "icon16.png",
        	"32": "icon32.png",
        	"48": "icon48.png",
		"128": "icon128.png"
	},
	"options_page": "options.html"
}
